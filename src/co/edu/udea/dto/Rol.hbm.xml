<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Archivo de mapeo generado por Eclipse para el pojo de Rol-->
<hibernate-mapping>
    <class name="co.edu.udea.dto.Rol" table="roles">
    
        <id name="codigo" type="java.lang.String"> 
            <column name="CODIGO"  length="12"/> 
            <generator class="assigned" />
        </id>
        
        <property name="nombre" type="java.lang.String">
  			<column name="NOMBRE" length="35" not-null="true" /> 
        </property>
    </class>
</hibernate-mapping>
